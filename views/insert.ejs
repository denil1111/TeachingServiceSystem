        <p>
          <span>姓名</span>
          <br>
          <input id="username" name="username" placeholder="your name" type="text">
        </p>
        <p>
            <span>身份：</span>
            <br>
            <select id="degree" name="degree">
              <optgroup lable="管理员">
                <option value="系统管理员" title ="请确认您是否有相应权限">系统管理员</option>
                <option value="教务管理员">教务管理员</option>
              </optgroup>
              <optgroup lable="用户">
                <option value="教师">教师</option>
                <option value="学生" selected="selected">学生</option>
              </optgroup>
            </select>           
            <!--input id="password" name="password" type="password"-->
        </p>
        <p>
          <span>性别：</span>
          <br>
          <select id="sex" name="sex">
            <option value="男">男</option>
            <option value="女">女</option>
          </select>
        </p>
        <p>
          <span>学院：</span>
          <br>
          <input id="college" name="college" type="text">
        </p>
        <p>
          <span>Tel：</span>
          <br>
          <input id="tel" name="tel" type="text">
        </p>
        <p>
          <span>Email：</span>
          <br>
          <input id="email" name="email" type="text">
        </p>
        <p><input type="submit" onclick="insert();" value="submit"></p>
    </form>
