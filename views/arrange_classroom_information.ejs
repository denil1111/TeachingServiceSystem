<!doctype html>
<% include head.html %>
</head>
<body>
<!-- 上方选择条 navbar-top-->
<% include navbar.html %>
<!-- 侧边框 -->
<% include arrange_sidebar.html%>
<script type="text/javascript">
   function setNavActive(){
       var active=document.getElementById('arrange');
           active.className='active';
    }

   function setSlideActive(){
        var slide = document.getElementById('arrange_classroom_information');
            slide.className='active';
   }

     setNavActive();
     setSlideActive();
</script>

<!-- 主要内容 -->
<div class="main-content">
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="area-top clearfix">
        <div class="pull-left header">
          <h3 class="title"><i class="icon-calendar"></i>教室信息</h3>
          <h5>查看你的课表</h5>
        </div>
      </div>
    </div>
  </div>
  <!-- 个人课表 -->
  <div class="span10">
    <div class="box">
      <div class="box-header">
        <span class="title">我的课表</span>
        <ul class="box-toolbar">
          <li><span class="label label-blue">2</span></li>
        </ul>
      </div>
      <div class="box-content scrollable" style="overflow-y:auto">
