<!DOCTYPE html>
<html>
  <head>
    <title>Person Insert</title>
    <meta charset="UTF-8">
  </head>
  <body>
    <h1>Sign In</h1>
    <form action="/info/personinsert" role="form" method="post">
        <p><span><%= insertresult %></span>
        </p>
        <p>
          <span>姓名</span>
          <br>
          <input id="username" name="username" placeholder="your name" type="text">
        </p>
        <p>
            <span>身份：</span>
            <br>
            <select id="status" name="status">
              <optgroup lable="管理员">
                <option value="系统管理员" title ="请确认您是否有相应权限">系统管理员</option>
                <option value="教务管理员">教务管理员</option>
              </optgroup>
              <optgroup lable="用户">
                <option value="教师">教师</option>
                <option value="学生" selected="selected">学生</option>
              </optgroup>
            </select>           
            <!--input id="password" name="password" type="password"-->
        </p>
        <p>
          <span>课程</span>
          <br>
          <div id="org"></div>
          <input type="button" onclick="add1();" value="添加" />
        </p>
        <p>
          <span>性别：</span>
          <br>
          <select id="sex" name="sex">
            <option value="男">男</option>
            <option value="女">女</option>
          </select>
        </p>
        <p>
          <span>年龄</span>
          <br>
          <input id="age" name="age" type="text">
        </p>
        <p>
          <span>专业：</span>
          <br>
          <input id="major" name="major" type="text">
        </p>
        <p>
          <span>学院：</span>
          <br>
          <input id="college" name="college" type="text">
        </p>
        <p>
          <span>职称：</span>
          <br>
          <input id="title" name="title" type="text">
        </p>
        <p>
          <span>Tel：</span>
          <br>
          <input id="tel" name="tel" type="text">
        </p>
        <p>
          <span>Email：</span>
          <br>
          <input id="email" name="email" type="text">
        </p>
        <p><input type="submit" onclick="personinsert();" value="submit"></p>
    </form>
    script
    <script type="text/javascript">
    function add1(){
      var input1 = document.createElement('input');
      input1.setAttribute('type', 'text');
      input1.setAttribute('name', 'organizers');//order?
      input1.setAttribute('class', 'git');
      
      var btn1 = document.getElementById("org");
      btn1.insertBefore(input1,null);
    }
    </script> 
  </body>
</html>