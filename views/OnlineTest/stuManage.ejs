<!DOCTYPE html>
<html>
  <head>  
    <% include ../head.html %>
  </head>

  <body>
    <% include ../navbar.html %>
    <% include slide_student.html %>
    <div class="main-content">
       <div class="span12">
        <div class="box">
          <div class="box-header">
            <div class="title">试卷列表</div>
          </div>
          <div class="box-content">
            <ul>
            <% for(var i = 0; i < papers.length; i++){
                var paper = papers[i];
            %>
              <li>
              <a href="/OnlineTest/student/answer=<%= paper._id %>"><%= paper.title %></a>
              </li>
            <% } %>
            </ul>
          </div>
        </div>
      </div>

       <div class="span12">
         <div class="box">
           <div class="box-header">
             <div class="title">历史成绩</div>
           </div>
           <div class="box-content">
             <ul>
               <% for(var i = 0; i < records.length; i++){
                   var record = records[i];
               %>
                 <li>
                 《<%= titles[i] %>》 得分：<%= record.point %>
                 </li>
               <% } %>
             </ul>
           </div>
         </div>
       </div>
     </div>
  </body>
</html>